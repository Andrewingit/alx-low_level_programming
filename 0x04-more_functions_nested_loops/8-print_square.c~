#include <stdio.h>
#include "main.h"

/**
*print_square - prints squares
*@size: parameter
*/

void print_square(int size)
{
int incl1, incl2;

if (size > 0)
{
for (inc1 = 0; inc1 < size; inc1++)
{
for(inc2 = 0; inc2 < (size - 1); inc2++)
{
putchar('#');
}
putchar('#');

putchar('\n');
}
}
else
{
putchar('\n');
}
}
